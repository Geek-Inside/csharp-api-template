{
  "ConnectionStrings": {
    "CORE_CONNECTION_STRING": "Server=localhost;Port=5432;Database=api-template;User Id=postgres;Password=111111;"
  },
  "Serilog": {
    "Properties:Application": "api-template",
    "Using": [ "Serilog.Sinks.Async", "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "Enrich": [ "WithProcessId", "WithProcessName", "WithThreadId", "WithThreadName", "FromLogContext" ],
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      },
      {
        "Name": "Async",
        "Args": {
          "bufferSize": 100,
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "api-template.log",
                "shared": true,
                "rollingInterval": "Day",
                "rollOnFileSizeLimit": true,
                "retainedFileCountLimit": null
              }
            }
          ]
        }
      }
    ]
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://127.0.0.1:8005"
      }
    }
  },
  "Setup": {
    
  }
}
